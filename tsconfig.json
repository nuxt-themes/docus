{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxt/content",
      "nuxt-i18n",
      "nuxt-windicss"
    ]
  },
  "exclude": ["node_modules"]
}
