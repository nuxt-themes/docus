{
  "name": "@nuxt-themes/docus",
  "version": "1.1.5",
  "type": "module",
  "main": "./nuxt.config.ts",
  "files": [
    "app",
    "assets",
    "components",
    "composables",
    "layouts",
    "plugins",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "tokens.config.ts"
  ],
  "scripts": {
    "prepare": "nuxi prepare && nuxi prepare .docs && nuxi prepare .playground",
    "play": "nuxi dev .playground/",
    "dev": "nuxi dev .docs/",
    "build": "nuxi build .docs/",
    "generate": "nuxi generate .docs/",
    "preview": "nuxi preview .docs/",
    "lint": "eslint .",
    "release": "release-it"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.0",
    "@docsearch/js": "^3.3.0",
    "@fontsource/inter": "^4.5.14",
    "@iconify/vue": "^4.0.2",
    "@nuxt-themes/elements": "^0.2.2",
    "@nuxt-themes/tokens": "latest",
    "@nuxt-themes/typography": "latest",
    "@nuxt/content": "^2.2.2",
    "@nuxthq/studio": "^0.3.5",
    "@nuxtjs/algolia": "^1.3.1",
    "@nuxtjs/color-mode": "^3.2.0",
    "@vueuse/core": "^9.6.0",
    "@vueuse/nuxt": "^9.6.0",
    "nuxt-config-schema": "^0.3.6",
    "pinceau": "latest"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.2",
    "@docsearch/css": "^3.3.0",
    "@docsearch/js": "^3.3.0",
    "@nuxtjs/algolia": "^1.3.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.29.0",
    "nuxt": "3.0.0",
    "nuxt-plausible": "^0.1.2",
    "release-it": "^15.5.1",
    "typescript": "^4.9.4",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios"
      ]
    }
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm build"
      ]
    },
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
