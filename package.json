{
  "name": "@nuxt-themes/docus",
  "version": "0.2.1",
  "type": "module",
  "main": "./nuxt.config.ts",
  "files": [
    "app",
    "assets",
    "components",
    "composables",
    "layouts",
    "plugins",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "tokens.config.ts"
  ],
  "scripts": {
    "prepare": "nuxi prepare && nuxi prepare .docs && nuxi prepare .playground",
    "play": "nuxi dev .playground/",
    "dev": "nuxi dev .docs/",
    "build": "nuxi build .docs/",
    "generate": "nuxi generate .docs/",
    "preview": "nuxi preview .docs/",
    "lint": "eslint .",
    "release": "standard-version && git push --follow-tags && npx npm publish"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.0",
    "@docsearch/js": "^3.3.0",
    "@fontsource/inter": "^4.5.14",
    "@iconify/vue": "^4.0.0",
    "@nuxt-themes/typography": "^0.0.12",
    "@nuxt/content": "^2.2.2",
    "@nuxthq/studio": "^0.2.3",
    "@nuxtjs/algolia": "^1.1.5",
    "@nuxtjs/color-mode": "^3.1.8",
    "@vueuse/core": "^9.5.0",
    "@vueuse/nuxt": "^9.5.0",
    "pinceau": "latest"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.2",
    "@docsearch/css": "^3.3.0",
    "@docsearch/js": "^3.3.0",
    "@nuxtjs/algolia": "^1.1.5",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtlabs/github-module": "npm:@nuxtlabs/github-module-edge@latest",
    "eslint": "^8.27.0",
    "nuxt": "npm:nuxt3@latest",
    "nuxt-plausible": "^0.1.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.3",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios"
      ]
    }
  }
}
