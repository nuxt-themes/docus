{
  "name": "@nuxt-themes/docus",
  "version": "2.0.0-beta.13",
  "type": "module",
  "main": "./nuxt.config.ts",
  "packageManager": "pnpm@8.5.1",
  "repository": "nuxt-themes/docus",
  "files": [
    "app",
    "assets",
    "components",
    "composables",
    "layouts",
    "plugins",
    "nuxt.schema.ts",
    "server",
    "app.vue",
    "app.config.ts",
    "nuxt.config.ts",
    "tokens.config.ts"
  ],
  "scripts": {
    "prepare": "nuxi prepare && nuxi prepare .starters/default",
    "dev": "nuxi dev .starters/default",
    "build": "nuxi build .starters/default",
    "generate": "nuxi generate .starters/default",
    "preview": "nuxi preview .starters/default",
    "lint": "eslint .",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt/content": "^2.8.2",
    "@nuxthq/studio": "^0.14.1",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@tailwindcss/typography": "^0.5.10",
    "@vueuse/integrations": "^10.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "focus-trap": "^7.5.3",
    "fuse.js": "^6.6.2",
    "nuxt-icon": "^0.5.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.8.5",
    "@nuxt/eslint-config": "^0.2.0",
    "@types/node": "^20.6.5",
    "eslint": "^8.50.0",
    "nuxt": "^3.7.3",
    "release-it": "^16.2.0",
    "typescript": "^5.2.2",
    "vite-plugin-inspect": "^0.7.38",
    "vue": "^3.3.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0",
        "vite": "*"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios",
        "vite"
      ]
    }
  },
  "license": "MIT",
  "release-it": {
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
